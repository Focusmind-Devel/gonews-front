	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"react-snapshot": "^1.3.0"
	},
	"reactSnap": {
		"inlineCss": true,
		"cacheAjaxRequests": true,
		"minifyHtml": {
			"collapseWhitespace": false,
			"removeComments": false
		},
		"puppeteerArgs": [
			"--no-sandbox",
			"--disable-setuid-sandbox"
		]
	},
	"puppeteer": {
		"cache": false
	}
